<Defs>
  <ThingDef ParentName="ApparelBase">
    <defName>BaseShoe</defName>
    <label>holliday shoes</label>
    <description>Nice new shoes!</description>
    <techLevel>Spacer</techLevel>
    <useHitPoints>True</useHitPoints>
    <graphicData>
      <texPath>BaseShoe</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>400</MarketValue>
      <DeteriorationRate>0</DeteriorationRate>
      <MaxHitPoints>3400</MaxHitPoints>
      <Mass>1</Mass>
      <ArmorRating_Blunt>0.1</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.1</ArmorRating_Sharp>
      <ArmorRating_Heat>0.1</ArmorRating_Heat>
      <Insulation_Cold>1</Insulation_Cold>
      <Insulation_Heat>1</Insulation_Heat>
      <EquipDelay>1</EquipDelay>
    </statBases>
    <tradeability>Sellable</tradeability>
    <equippedStatOffsets>
      <MoveSpeed>0.10</MoveSpeed>
    </equippedStatOffsets>
    <costList>
      <WoodLog>2</WoodLog>
    </costList>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
    <generateCommonality>0</generateCommonality>
    <apparel>
      <bodyPartGroups>
        <li>Feet</li>
      </bodyPartGroups>
      <layers>
        <li>OnSkin</li>
      </layers>
      <tags>
        <li>Outlander</li>
        <li>Spacer</li>
      </tags>
      <defaultOutfitTags>
        <li>Worker</li>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
  </ThingDef>
  <!--============================ Trees ============================-->
  <ThingDef ParentName="TreeBase" Name="HollidayTreeBase" Abstract="True">
    <statBases>
      <MaxHitPoints>300</MaxHitPoints>
      <Nutrition>1.5</Nutrition>
    </statBases>
    <description>A holliday tree.</description>
    <altitudeLayer>Building</altitudeLayer>
    <selectable>true</selectable>
    <fillPercent>0.25</fillPercent>
    <graphicData>
      <shadowData>
        <volume>(0.2, 0.35, 0.13)</volume>
        <offset>(0,0,-0.42)</offset>
      </shadowData>
    </graphicData>
    <passability>PassThroughOnly</passability>
    <pathCost>130</pathCost>
    <blockWind>true</blockWind>
    <ingestible>
      <foodType>Tree</foodType>
      <preferability>RawBad</preferability>
    </ingestible>
    <plant>
      <fertilityMin>0.3</fertilityMin>
      <fertilitySensitivity>0.5</fertilitySensitivity>
      <soundHarvesting>Harvest_Tree</soundHarvesting>
      <soundHarvestFinish>Harvest_Tree_Finish</soundHarvestFinish>
